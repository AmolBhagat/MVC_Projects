@model IEnumerable<MvcCrud1.Models.Contact>
@{
    ViewBag.Title = "ContactList";
}

<h2>ContactList</h2>
<p>
    @Html.ActionLink("Create New","Create")
</p>

<table class="table" border="1">
    <tr>
        <th>Contactname</th>
        <th>PhoneNumber1</th>
        <th>PhoneNumber2</th>
        <th>Address1</th>
        <th>Address2</th>
        <th>Birthdate</th>
        <th>EmailID</th>
        <th>Active(Y/N)</th>
        <th></th>
    </tr>

    @foreach(var item in Model)
    {
        <tr>
            <td>@item.ContactName</td>
            <td>@item.PhoneNumber1</td>
            <td>@item.PhoneNumber2</td>
            <td>@item.Address1</td>
            <td>@item.Address2</td>
            <td>@item.Birthdate</td>
            <td>@item.EmailID</td>
            <td>@item.Active_Y_N_</td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ContactID }) |
                @Html.ActionLink("Details", "Details", new { id = item.ContactID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ContactID })
            </td>
            
        </tr>
    }
</table>